# Example secret for GitLab Runner registration token
# 
# This file is for reference only. Create the actual secret using kubectl:
#
#   kubectl create secret generic gitlab-runner-secret \
#     --from-literal=runner-registration-token='<YOUR_RUNNER_TOKEN>' \
#     -n managed-cicd

apiVersion: v1
kind: Secret
metadata:
  name: gitlab-runner-secret
  namespace: managed-cicd
type: Opaque
stringData:
  runner-registration-token: "<YOUR_GITLAB_RUNNER_TOKEN>"
